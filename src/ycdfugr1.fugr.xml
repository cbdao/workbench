     <FUNCNAME>YCD_GET_PRICES</FUNCNAME>
     <SHORT_TEXT>get prices for a given object type</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>OBJ_TYPE</PARAMETER>
       <REFERENCE>X</REFERENCE>
      </RSIMP>
      <RSIMP>
       <PARAMETER>OBJ_NAME</PARAMETER>
       <REFERENCE>X</REFERENCE>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>PRICE</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>KBETR</TYP>
      </RSEXP>
      <RSEXP>
       <PARAMETER>CURRENCY</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>WAERS</TYP>
      </RSEXP>
     </EXPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>NOTFOUND</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>OBJ_TYPE</PARAMETER>
       <KIND>P</KIND>
      </RSFDO>
      <RSFDO>
       <PARAMETER>OBJ_NAME</PARAMETER>
       <KIND>P</KIND>
      </RSFDO>
      <RSFDO>
       <PARAMETER>PRICE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Rate (condition amount or percentage)</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>CURRENCY</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Currency Key</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>NOTFOUND</PARAMETER>
       <KIND>X</KIND>
      </RSFDO>
     </DOCUMENTATION>
    </item>
